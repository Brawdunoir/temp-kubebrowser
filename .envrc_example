# Automatically sets up your devbox environment whenever you cd into this
# First you need to install direnv hook into your shell

# eval "$(devbox generate direnv --print-envrc)"

export SKAFFOLD_KUBE_CONTEXT=minikube
export KUBEBROWSER_OAUTH2_CLIENT_ID=<your-client-id>
export KUBEBROWSER_OAUTH2_CLIENT_SECRET=<your-client-secret>
export KUBEBROWSER_OAUTH2_ISSUER_URL=<your-issuer-url>
export KUBEBROWSER_DEV=true
export KUBEBROWSER_LOG_LEVEL=DEBUG
